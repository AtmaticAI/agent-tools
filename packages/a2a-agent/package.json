  },
  "dependencies": {
    "@agent-tools/core": "workspace:*",
    "uuid": "^11.0.0"
  },
  "devDependencies": {
    "@types/uuid": "^10.0.0",
    "pdf-lib": "^1.17.0",
    "tsup": "^8.3.0",
    "typescript": "^5.7.0",
    "vitest": "^3.2.0"
  }
